{
  "name": "com.example.installment",
  "displayName": "installment",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf www && mkdir www && cd front-end && yarn build",
    "start": "cd front-end && yarn start",
    "android": "cordova run android",
    "ios": "cordova run ios",
    "browser": "cordova run browser",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "pm2 start ./back-end/server.js",
    "end": "pm2 stop ./back-end/server.js"
  },
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "cordova-android": "^7.1.4",
    "cordova-browser": "^5.0.4",
    "cordova-ios": "^5.1.1",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.4",
    "koa2-cors": "^2.0.6"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {}
    },
    "platforms": [
      "android",
      "browser",
      "ios"
    ]
  }
}